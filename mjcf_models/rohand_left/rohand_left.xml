<!-- 
=================================================================================
RoHand 左手 MuJoCo 物理仿真模型
=================================================================================
本文件定义了一个高精度的机器人左手模型，包含5个手指的完整运动学和动力学特性。
模型特点：
- 25个自由度关节 (每个手指5个关节)
- 5个集成腱驱动系统 (实现手指协调弯曲，包含外展关节耦合)
- 6个执行器 (5个弯曲 + 1个拇指旋转)
- 完整的惯性属性和碰撞检测
- 自然的抓取动作 (弯曲时自动收拢)
=================================================================================
-->
<mujoco model="rohand_left">
  
  <!-- =================================================================================
   编译器设置: 定义角度单位和坐标系统
   ================================================================================= -->
  <compiler angle="radian" />  <!-- 使用弧度制，符合机器人学标准 -->
  
  <!-- =================================================================================
   资源定义: 加载所有STL网格文件，定义手部各个关节的3D几何形状
   ================================================================================= -->
  <asset>
    <!-- 手掌基座 -->
    <mesh name="base_link" content_type="model/stl" file="base_link.STL" />
    
    <!-- 食指 (Index Finger) 网格文件 -->
    <mesh name="if_slider_link" content_type="model/stl" file="if_slider_link.STL" />        <!-- 食指滑块部件 -->
    <mesh name="if_slider_abpart_link" content_type="model/stl" file="if_slider_abpart_link.STL" /> <!-- 食指外展部件 -->
    <mesh name="if_proximal_link" content_type="model/stl" file="if_proximal_link.STL" />     <!-- 食指近端指节 -->
    <mesh name="if_distal_link" content_type="model/stl" file="if_distal_link.STL" />         <!-- 食指远端指节 -->
    <mesh name="if_connecting_link" content_type="model/stl" file="if_connecting_link.STL" />  <!-- 食指连接部件 -->
    
    <!-- 中指 (Middle Finger) 网格文件 -->
    <mesh name="mf_slider_link" content_type="model/stl" file="mf_slider_link.STL" />        <!-- 中指滑块部件 -->
    <mesh name="mf_slider_abpart_link" content_type="model/stl" file="mf_slider_abpart_link.STL" /> <!-- 中指外展部件 -->
    <mesh name="mf_proximal_link" content_type="model/stl" file="mf_proximal_link.STL" />     <!-- 中指近端指节 -->
    <mesh name="mf_distal_link" content_type="model/stl" file="mf_distal_link.STL" />         <!-- 中指远端指节 -->
    <mesh name="mf_connecting_link" content_type="model/stl" file="mf_connecting_link.STL" />  <!-- 中指连接部件 -->
    
    <!-- 无名指 (Ring Finger) 网格文件 -->
    <mesh name="rf_slider_link" content_type="model/stl" file="rf_slider_link.STL" />        <!-- 无名指滑块部件 -->
    <mesh name="rf_slider_abpart_link" content_type="model/stl" file="rf_slider_abpart_link.STL" /> <!-- 无名指外展部件 -->
    <mesh name="rf_proximal_link" content_type="model/stl" file="rf_proximal_link.STL" />     <!-- 无名指近端指节 -->
    <mesh name="rf_distal_link" content_type="model/stl" file="rf_distal_link.STL" />         <!-- 无名指远端指节 -->
    <mesh name="rf_connecting_link" content_type="model/stl" file="rf_connecting_link.STL" />  <!-- 无名指连接部件 -->
    
    <!-- 小指 (Little Finger) 网格文件 -->
    <mesh name="lf_slider_link" content_type="model/stl" file="lf_slider_link.STL" />        <!-- 小指滑块部件 -->
    <mesh name="lf_slider_abpart_link" content_type="model/stl" file="lf_slider_abpart_link.STL" /> <!-- 小指外展部件 -->
    <mesh name="lf_proximal_link" content_type="model/stl" file="lf_proximal_link.STL" />     <!-- 小指近端指节 -->
    <mesh name="lf_distal_link" content_type="model/stl" file="lf_distal_link.STL" />         <!-- 小指远端指节 -->
    <mesh name="lf_connecting_link" content_type="model/stl" file="lf_connecting_link.STL" />  <!-- 小指连接部件 -->
    
    <!-- 拇指 (Thumb) 网格文件 -->
    <mesh name="th_root_link" content_type="model/stl" file="th_root_link.STL" />             <!-- 拇指根部 -->
    <mesh name="th_proximal_link" content_type="model/stl" file="th_proximal_link.STL" />     <!-- 拇指近端指节 -->
    <mesh name="th_slider_link" content_type="model/stl" file="th_slider_link.STL" />         <!-- 拇指滑块部件 -->
    <mesh name="th_connecting_link" content_type="model/stl" file="th_connecting_link.STL" />  <!-- 拇指连接部件 -->
    <mesh name="th_distal_link" content_type="model/stl" file="th_distal_link.STL" />         <!-- 拇指远端指节 -->
  </asset>
  
